    "test": "DEBUG=ifls:* jest",
    "test:watch": "DEBUG=ifls:* jest --watch",
    "test:coverage": "DEBUG=ifls:* jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamiegyoung/IFLS.git"
