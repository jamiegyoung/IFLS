  },
  "homepage": "https://github.com/jamiegyoung/IFLS#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@types/debug": "^4.1.7",
    "@types/flat-cache": "^2.0.0",
    "@types/glob": "^7.2.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-jest": "^28.1.3",
    "eslint": "^8.21.0",
