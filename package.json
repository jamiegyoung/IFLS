{
  "name": "ifls",
  "version": "1.0.0",
  "description": "I'm feeling lucky script",
  "main": "index.js",
  "bin": {
    "iflsc": "./bin/iflsc"
  },
  "scripts": {
    "build": "tsc; ./bin/iflsc -o . . ",
    "start": "tsc; node lib/index.js",
    "dev": "tsc; DEBUG=ifls:* node lib/index.js",
